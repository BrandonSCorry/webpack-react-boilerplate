{
  "name": "webpack-boilerplate",
  "version": "0.1.0",
  "description": "Essential Webpack Boilerplate for React",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiayihu/webpack-boilerplate.git"
  },
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.prod.babel.js",
    "clean": "rm -rf dist",
    "start": "webpack-dev-server --inline --hot --config webpack.config.dev.babel.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tree": "tree -d -I node_modules > tree.txt"
  },
  "keywords": [
    "webpack",
    "boilerplate",
    "react"
  ],
  "author": "Jiayi Hu",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.6.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
